<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.mawqey.qontacts" android:versionName="@string/version_name" android:versionCode="1">
    <application android:label="@string/app_name" android:description="@string/app_description" android:icon="@drawable/icon48">
        <activity android:label="@string/app_name" android:name="Qontacts">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
	
    <activity android:name="com.mawqey.qontacts.activity.ContactList" android:label="@string/contacts_list"></activity>
<activity android:name="com.mawqey.qontacts.activity.ContactView" android:label="@string/preview"></activity>
<activity android:name="com.mawqey.qontacts.activity.AboutView" android:label="@string/about_qontacts"></activity>

</application>

<uses-sdk android:minSdkVersion="3"/>

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
<uses-permission android:name="android.permission.WRITE_CONTACTS"></uses-permission>







</manifest> 